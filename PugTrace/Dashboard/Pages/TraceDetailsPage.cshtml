@* Generator: Template TypeVisibility: Internal GeneratePrettyNames: True *@
@using PugTrace.Dashboard
@using PugTrace.Dashboard.Pages
@using PugTrace.Storage
@inherits RazorPage<TraceData>
@{
    Layout = new LayoutPage();
}
<dl class="dl-horizontal">
    <dt>Application</dt>
    <dd>@Model.ApplicationName</dd>
    <dt>Source</dt>
    <dd>@Model.Source</dd>
    <dt>Id</dt>
    <dd>@Model.Id</dd>
    <dt>Event Type</dt>
    <dd>@Model.EventType</dd>
    <dt>Timestamp</dt>
    <dd>@Model.UtcDateTime.ToString("f")</dd>
    <dt>Machine</dt>
    <dd>@Model.MachineName</dd>
    @if (!string.IsNullOrEmpty(Model.Message))
    {
        <dt>Message</dt>
        <dd>@Model.Message</dd>
    }
</dl>

@Html.RenderData(Model.Data)